enclave {
    from "openenclave/edl/syscall.edl" import *;
    from "openenclave/edl/socket.edl" import *;
    from "platform.edl" import *;

    trusted {
        public void socket_test(uint16_t port);
        public void initialize_enclave() transition_using_threads;
        public void connect_enclave(const char* server_host, const char* server_port) transition_using_threads;
        public void request_enclave(const char* http_request) transition_using_threads;
        public void receive_enclave(char* buf, int len) transition_using_threads;
        public void close_enclave() transition_using_threads;
    };

    untrusted {
    };
};
